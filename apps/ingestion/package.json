{
  "name": "@agora/ingestion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "vercel dev",
    "build": "node ../../node_modules/typescript/bin/tsc",
    "ingest": "ts-node src/ingest.ts",
    "ingest:scrutins": "ts-node src/ingest-scrutins-cli.ts",
    "ingest:deputies": "ts-node src/ingest-deputies-cli.ts",
    "ingest:circonscriptions": "ts-node src/ingest-circonscriptions-cli.ts",
    "tag:all-scrutins": "ts-node src/tag-all-scrutins-cli.ts",
    "tag:single-scrutin": "ts-node src/tag-single-scrutin-cli.ts",
    "lint": "eslint . && node ../../node_modules/typescript/bin/tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@agora/shared": "*",
    "@supabase/supabase-js": "^2.48.1",
    "dotenv": "^16.6.1",
    "node-fetch": "^3.3.2",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/unzipper": "^0.10.10",
    "@vercel/node": "^5.3.21",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  }
}
